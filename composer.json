{
  "name": "simivar/tibiadata-api",
  "description": "TibiaData API is a open source library that allows you to access TibiaData API from your PHP application. Supports every resource. Follows Semver and TibiaData API versioning.",
  "keywords": ["tibia", "tibiadata", "api", "psr-4", "httplug"],
  "homepage": "https://github.com/simivar/tibiadata-api",
  "version": "2.3.2",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "simivar",
      "email": "simivar@gmail.com",
      "homepage": "https://github.com/simivar/tibiadata-api"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "php-http/httplug": "^1.0",
    "php-http/message": "^1.0",
    "php-http/client-common": "^1.0",
    "php-http/discovery": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "^7",
    "php-http/guzzle6-adapter": "^1.0",
    "php-http/message": "^1.0"
  },
  "autoload": {
    "psr-4": {"TibiaDataApi\\": "src/TibiaDataApi"}
  },
  "autoload-dev": {
    "psr-4": {"TibiaDataApi\\Test\\": "tests"}
  }
}
